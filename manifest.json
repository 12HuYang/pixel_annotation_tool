{
  "manifest_version": 2,
  "name": "Annotation Tool",
  "description": "This extension for generate fetch script",
  "version": "1.4",
  "background": {
    "scripts": ["./build/background.js"]
  },
  "browser_action": {
    "default_icon": "./icon.png",
    "default_title": "标注"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking"
  ]
}